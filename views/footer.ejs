    <footer class="page-footer dark">
      <section class="clean-block about-us" style="padding:0">
        <% if (typeof user !== 'undefined' && user !== null) { %>
        <div class="container" data-aos="fade">
            <div class="block-heading" style="padding:0">
                <h2 class="text-info">Thành viên gần đây</h2>
            </div>
            <div class="row user-list">
              <% for (let i of status) { %>
                <div class="col-12 col-sm-6 col-md-4 col-lg-3 user-item">
                    <% let src = (i.avatarFile == undefined ? "assets/img/avatars/1.jpg" : i.avatarFile) %>
                    <div class="user-container"><a class="user-avatar userdmml" href="#"><img class="rounded-circle" src="<%= src %>" width="48" height="48" alt="Profile of Mark Smith Peterson"></a>
                        
                        <p class="user-name"><a href="#" class="userlinkdm userdmmle" style="color: black;" onmouseover="this.style.color='blue'" onmouseout="this.style.color='black'"><%= i.username %></a><span>
                        <% if (typeof i.socket !== 'undefined' && i.socket !== null) { %>
                          Online</span></p>
                          <a class="user-online"> </a>
                        <% } else { %>
                          Offline</span></p>
                          <a class="user-offline"> </a>
                        <% } %>
                    </div>
                </div>
              <% } %>
            </div>
        </div>
        <% } %>
    </section>
        <div class="footer-copyright">
            <p>© 2020 Tamkien Cao</p>
        </div>
    </footer>
    <script>
      for (let i of document.getElementsByClassName("userdmmle"))
      i.setAttribute("href", "/users/" + i.textContent)
    </script>
